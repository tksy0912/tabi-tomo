<div class="nested-fields">
  <div class="form-group">
    <%= f.label :date, class: "form-label" %>
    <%= f.text_field :date, class: 'form-control' %>
  </div>
  <%= f.label :date %>: <%= f.text_field :date %>
  <%= f.label :destination %>: <%= f.text_field :destination %>
  <%= f.label :transportation %>: <%= f.text_field :transportation %>
  <%= f.label :is_accommodation %>: <%= f.check_box :is_accommodation %>
  <%= f.label :accommodation_name %>: <%= f.text_field :accommodation_name %>
  <%= link_to_remove_association "delete", f %>
  <hr>
</div>