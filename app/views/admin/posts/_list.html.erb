<h4>投稿一覧</h4>
<table class="table table-borderless">
  <thead style="background-color: #aeeaef;">
    <tr>
      <th>タイトル</th>
      <th>投稿日</th>
    </tr>
  </thead>
  <tbody>
  <% @posts.each do |post| %>
    <tr>
      <td class="align-middle"><%= link_to post.title, admin_post_path(post.id) %></td>
      <td class="align-middle"><%= post.created_at.to_date %></td>
    </tr>
  <% end %>
  </tbody>
</table>